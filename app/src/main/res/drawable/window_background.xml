<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:drawable="@color/md_theme_background" />
    <item
        android:width="200dp"
        android:height="200dp"
        android:gravity="top|end"
        android:right="-75dp"
        android:top="-75dp">
        <shape android:shape="oval">
            <size
                android:width="200dp"
                android:height="200dp" />
            <solid android:color="@color/md_theme_primaryContainer_alpha" />
        </shape>
    </item>
    <item
        android:width="200dp"
        android:height="200dp"
        android:gravity="top|end"
        android:right="-100dp"
        android:top="-100dp">
        <shape android:shape="oval">
            <size
                android:width="200dp"
                android:height="200dp" />
            <solid android:color="@color/md_theme_primaryContainer" />
        </shape>
    </item>
</layer-list>